<!-- panel-all-sessions-overview -->
<div class="panel">
    {{#each sessions as |session|}}
        <div class="card">
            <div class="card-header">
                <h5>
                    <button class="btn btn-link" data-toggle="collapse">
                        {{session.date}}
                    </button>
                </h5>
            </div>
        </div>>
        <div class="collapse show">
            <div class="card-body">
                Example text for now.
            </div>
        </div>
    {{else}}
        <p>Oops! Looks like you haven't logged any exercise sessions!</p>
    {{/each}}
</div>

