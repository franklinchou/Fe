<!-- panel-session-overview -->
<div class="card">
  <div class="card-header" id={{concat 'heading' index}}>
    <h5 class="mb-0">
      <button 
        class="btn btn-link" 
        data-toggle="collapse"
        data-target={{concat '#collapse' index}}
        aria-expanded="false" 
        aria-controls={{concat 'collapse' index}}
      >
        {{session.date}}
      </button>
    </h5>
  </div>
  <div 
    id={{concat 'collapse' index}}
    class="collapse"
    aria-labelledby={{concat 'heading' index}}
    data-parent="#accordian"
  >
    <div class="card-body">
      {{session.sets}}
    </div>
  </div>
</div>
